<h3 class="page-header">下書き</h3>
  <ul class="list-group" style="list-style: none;">
    <% @post.each do |post| %>
      <li><%= image_tag(post.image_url, width: 100) if post.image? %></li>
      <li><%= post.title%></li>
      <li><%= post.body.truncate(20)%></li>
      <li><%= button_to "[削除]","#",method: :delete %></li>
      <li><%= button_to "編集する", "#"%></li>
    <% end %>
  </ul>